{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/utils": ["src/lib/utils.ts"],
      "@lib/*": ["src/lib/*"],
      "@types": ["src/types.ts"],
      "@consts": ["src/consts.ts"]
    }
  }
}

